{
  "name": "dahu",
  "displayName": "dahu",
  "description": "advanced vscode colorscheme manager",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dahu.start-webview",
        "title": "Open webview"
      },
      {
        "command": "dahu.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && npm run build && tsc -p tsconfig.extension.json",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",

    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --output-hashing none && tsc -p tsconfig.extension.json",
    "e2e": "ng e2e",
    "package": "vsce package --yarn"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",


    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/language-service": "^17.0.8",
    "prettier": "^3.1.1",
    "ts-node": "~10.9.2",
    "vsce": "^2.15.0"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.3.9",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "rxjs": "~7.8.1",
    "vscode-test": "^1.6.1",
    "zone.js": "~0.14.2"
  }
}
